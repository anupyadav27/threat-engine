{
  "service": "sso",
  "version": "1.0",
  "kind_rules": {
    "read_list": ["List"],
    "read_get": ["Get", "Describe"],
    "write_create": ["Create", "Start", "Generate", "Import", "Enable", "Register"],
    "write_update": ["Update", "Modify", "Put", "Set", "Change", "Reset", "Patch"],
    "write_delete": ["Delete", "Remove", "Terminate", "Destroy", "Disable", "Detach", "Disassociate", "Untag", "Revoke"],
    "write_apply": ["Apply", "Attach", "Associate", "Add", "Tag", "Authorize", "Unauthorize", "Grant"],
    "other": ["default"]
  },
  "entity_aliases": {"sso.role_account_id": "sso.account_id"},
  "overrides": {},
  "operations": {
    "GetRoleCredentials": {
      "kind": "read_get",
      "side_effect": false,
      "sdk": {"client": "sso", "method": "get_role_credentials"},
      "consumes": [{"entity": "sso.role_name", "param": "roleName", "required": true, "source": "internal"}, {"entity": "sso.account_id", "param": "accountId", "required": true, "source": "internal"}, {"entity": "sso.access_token", "param": "accessToken", "required": true, "source": "external"}],
      "produces": [{"entity": "sso.role_credentials", "source": "output", "path": "roleCredentials"}, {"entity": "sso.role_credential_access_key_id", "source": "item", "path": "roleCredentials.accessKeyId"}, {"entity": "sso.role_credential_secret_access_key", "source": "item", "path": "roleCredentials.secretAccessKey"}, {"entity": "sso.role_credential_session_token", "source": "item", "path": "roleCredentials.sessionToken"}, {"entity": "sso.role_credential_expiration", "source": "item", "path": "roleCredentials.expiration"}],
      "notes": ""
    },
    "ListAccountRoles": {
      "kind": "read_list",
      "side_effect": false,
      "sdk": {"client": "sso", "method": "list_account_roles"},
      "consumes": [{"entity": "sso.access_token", "param": "accessToken", "required": true, "source": "external"}, {"entity": "sso.account_id", "param": "accountId", "required": true, "source": "internal"}],
      "produces": [{"entity": "sso.next_token", "source": "output", "path": "nextToken"}, {"entity": "sso.role_list", "source": "output", "path": "roleList"}, {"entity": "sso.role_name", "source": "item", "path": "roleList[].roleName"}, {"entity": "sso.account_id", "source": "item", "path": "roleList[].accountId"}],
      "notes": ""
    },
    "ListAccounts": {
      "kind": "read_list",
      "side_effect": false,
      "sdk": {"client": "sso", "method": "list_accounts"},
      "consumes": [{"entity": "sso.access_token", "param": "accessToken", "required": true, "source": "external"}],
      "produces": [{"entity": "sso.next_token", "source": "output", "path": "nextToken"}, {"entity": "sso.account_list", "source": "output", "path": "accountList"}, {"entity": "sso.account_id", "source": "item", "path": "accountList[].accountId"}, {"entity": "sso.account_name", "source": "item", "path": "accountList[].accountName"}, {"entity": "sso.account_email_address", "source": "item", "path": "accountList[].emailAddress"}],
      "notes": ""
    },
    "Logout": {
      "kind": "other",
      "side_effect": true,
      "sdk": {"client": "sso", "method": "logout"},
      "consumes": [{"entity": "sso.access_token", "param": "accessToken", "required": true, "source": "external"}],
      "produces": [],
      "notes": ""
    }
  }
}